{
  "name": "vschema-ui",
  "version": "1.3.7",
  "description": "A Vue 3 plugin for building dynamic UI from JSON Schema - declarative, reactive, and powerful",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "README.en.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "pnpm build && pnpm test",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "vue",
    "vue3",
    "json",
    "schema",
    "renderer",
    "dynamic",
    "component",
    "lowcode",
    "declarative",
    "ui",
    "vschema"
  ],
  "license": "MIT",
  "author": "maliang <maliang47@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maliang/vschema-ui.git"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.1.3",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.0",
    "fast-check": "^3.14.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "typescript": "~5.6.0",
    "vite": "^5.0.4",
    "vite-plugin-dts": "^3.6.4",
    "vitepress": "^1.6.4",
    "vitest": "^1.0.4",
    "vue": "^3.5.0",
    "vue-tsc": "^2.0.0"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
